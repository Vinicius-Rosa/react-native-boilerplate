{
  "expo": {
    "name": "Central Sa√∫de",
    "slug": "central-saude-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.centralsaude"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "packagerOpts": {
      "assetExts": [
        "bin",
        "txt",
        "jpg",
        "png"
      ],
      "sourceExts": [
        "js",
        "json",
        "ts",
        "tsx",
        "jsx",
        "bin"
      ]
    }
  }
}
